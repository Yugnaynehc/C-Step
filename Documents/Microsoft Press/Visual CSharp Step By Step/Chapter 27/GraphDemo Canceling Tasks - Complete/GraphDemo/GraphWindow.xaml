<Window x:Class="GraphDemo.GraphWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="Graph Demo" Height="343" Width="675">
    <Grid>
        <Image Margin="140,0,0,0" Name="graphImage" Stretch="Fill" />
        <Button Height="23" HorizontalAlignment="Left" Margin="12,38,0,0" Name="plotButton" VerticalAlignment="Top" Width="75" Click="plotButton_Click">Plot Graph</Button>
        <Label Height="28" HorizontalAlignment="Left" Margin="12,85,0,0" Name="duration" VerticalAlignment="Top" Width="122">Duration (ms):</Label>
        <Button Content="Cancel" Height="23" HorizontalAlignment="Left" Margin="12,135,0,0" Name="cancelButton" VerticalAlignment="Top" Width="75" Click="cancelButton_Click" />
        <Label Height="28" HorizontalAlignment="Left" Margin="12,190,0,0" Name="status" VerticalAlignment="Top" Width="120" />
    </Grid>
</Window>
