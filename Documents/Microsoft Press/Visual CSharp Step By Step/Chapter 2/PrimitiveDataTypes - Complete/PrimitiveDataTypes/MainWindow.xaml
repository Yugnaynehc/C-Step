<Window x:Class="PrimitiveDataTypes.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="Data Test" Height="269" Width="296">
    <Grid>
    <Label Height="33.2766666666667" Margin="50,14,51,0" Name="label1" VerticalAlignment="Top" FontSize="18">Primitive Data Types</Label>
    <Label Height="31" Margin="25,53,0,0" Name="label2" VerticalAlignment="Top" HorizontalAlignment="Left" Width="115.63">Choose a data type</Label>
    <Label Height="31" HorizontalAlignment="Right" Margin="0,53,17,0" Name="label3" VerticalAlignment="Top" Width="100">Sample value</Label>
    <ListBox Margin="25,91,143,26" SelectionMode="Single" Name="type" SelectionChanged="typeSelectionChange">
		<ListBoxItem>int</ListBoxItem>
		<ListBoxItem>long</ListBoxItem>
		<ListBoxItem>float</ListBoxItem>
		<ListBoxItem>double</ListBoxItem>
		<ListBoxItem>decimal</ListBoxItem>
		<ListBoxItem>string</ListBoxItem>
		<ListBoxItem>char</ListBoxItem>
		<ListBoxItem>bool</ListBoxItem>
	</ListBox>
    <TextBox Height="26" HorizontalAlignment="Right" Margin="0,90,17,0" Name="value" VerticalAlignment="Top" Width="100" />
    <Button Height="23" HorizontalAlignment="Right" Margin="0,0,42,26" Name="quit" VerticalAlignment="Bottom" Width="75" Click="quitClick">Quit</Button>
  </Grid>
</Window>
